<%- include ../layouts/header.ejs %>

<body>
    <%- include ../layouts/left-panel.ejs %>
    <!-- Right Panel -->
    <div id="right-panel" class="right-panel">
        <%- include ../layouts/header-panel.ejs %>
        <!-- Content -->
        <div class="content">
            <!-- Animated -->
            <div class="animated fadeIn">
                <!--  add  -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">Novo Or√ßamento</strong>
                            </div>
                            <div class="card-body">
                                <!-- new vehicle-->
                                <div id="add-vehicle">
                                    <div class="card-body">

                                        <!--  MSG ERRORS -->
                                        <% if (messages.error) { %>
                                            <p style="color:red"><%- messages.error %></p>
                                        <% } %>
                                        
                                        <% if (messages.success) { %>
                                            <p style="color:green"><%- messages.success %></p>
                                        <% } %>
                                        <form action="/budget/add" method="post" novalidate="novalidate">
                                            <div class="row">    
                                                <div class="col-3">
                                                    <div class="form-group">
                                                        <label for="license_plate" class="control-label mb-1">Placa</label>
                                                        <select id="license_plate" name="license_plate" class="form-control"></select>
                                                    </div>
                                                </div>
                                            </div> 
                                            <div class="row">
                                               
                                              
                                            </div>  
                                            <div class="row">
                                            </div>
                                        </form>
                                    </div>
                              
                                </div>
                            </div>
                        </div> <!-- .card -->
                    </div>
                </div>
                <!--  /add -->
                <div class="clearfix"></div>
            <!-- .animated -->
        </div>
        <!-- /.content -->
        <div class="clearfix"></div>
        <!-- Footer Copyright -->
        <%- include ../layouts/footer-copyright.ejs %>
        <!-- /.site-footer -->
    </div>
    <!-- /#right-panel -->
    </div>
    <!--JS FOR INIT SELECT2-->
    <script src="/js/jquery.min.js"></script>
